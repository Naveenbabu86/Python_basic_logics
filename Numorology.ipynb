    "# Numeraology Problem\n",
    "\n",
    "num = 2899\n",
    "num = str(num)\n",
    "length = len(num)\n",
    "while length > 1:\n",
    "    sum = 0\n",
    "    num = str(num)\n",
    "    for i in num:\n",
    "        sum += int(i)\n",
    "    num = sum\n",
    "    length = len(str(num))\n",
    "print(sum)\n",
    "\n",
    "    \n"
   ]
