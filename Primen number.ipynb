   "source": [
    "num = 9\n",
    "if num <= 1:\n",
    "    print(num, \"is not prime\")\n",
    "else:\n",
    "    for i in range(2, num):\n",
    "        print(\"i:\", i)\n",
    "        if num % i == 0:\n",
    "            print(num, \"is not prime\")\n",
    "            break\n",
    "    else:\n",
    "        print(num, \"is prime\")"
   ]
